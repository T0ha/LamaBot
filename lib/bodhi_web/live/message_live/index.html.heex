<.header>
  Listing Messages
</.header>

<.table
  id="chats"
  rows={@streams.messages}
>
  <:col :let={{_id, message}} label="Date">{message.date}</:col>
  <:col :let={{_id, message}} label="Text">{message.text}</:col>
  <:col :let={{_id, message}} label="Caption">{message.caption}</:col>
</.table>
